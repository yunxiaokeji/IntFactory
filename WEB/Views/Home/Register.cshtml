@{
    Layout = null;
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <meta charset="UTF-8">
    <meta content="供应链管理，智能工厂，流程管理，服装可视化管理" name="keywords">
    <meta content="中国市场最通用的智能工厂服装可视化管理平台，供应链管理，功能强大，2016年开始运营" name="description">
    <meta content="width=device-width,initial-scale=1,maximum-scale=1" name="viewport">
    <meta content="telephone=no" name="format-detection">
    <title>注册智能工厂</title>

    <link href="/modules/css/default/layout.css" rel="stylesheet" />
    <link href="/modules/css/home/login.css" rel="stylesheet" />
    <link href="/modules/css/base.css" rel="stylesheet" />
    <link rel="shortcut icon" href="/modules/images/logo_login.png">

    <script src="/Scripts/sea.js"></script>
    <script src="/Scripts/sea-config.js"></script>
    <script type="text/javascript">
        seajs.use(["scripts/home/register"], function (register) {
            register.init();
        });
    </script>
</head>
<body>
    <div class="login-body" >
     <div class="login-header">
            <div class="left header-logo" onclick="location.href='http://www.intfactory.cn/'">
                <div class="left"><img src="/modules/images/logo_login.png"  style="width:26px;"/></div>
                <div class="left headerTitle">厂盟科技</div>
                <div class="clear"></div>
            </div>
            <a class="btnRegister right" href="/Home/Login">登录</a>
       </div>

        <div class="main" style="margin-top:80px;">
            <div class="loginTitle">注册</div>

            <div class="login">
                <div class="registerErr hide"></div>

                <div class="loginbar">
                    <input class="input icoUserName" id="loginName" type="text" placeholder="手机号" value="" maxlength="25"/>
                    <div class="error-body">
                        <div class="error-lump"></div>
                        <div class="error-msg"></div>
                    </div>
                </div>

                <div class="loginbar">
                    <div class="left">
                        <input class="input icoMobileCode" id="code" style="width:200px;" type="text"  placeholder="验证码" />
                    </div>
                    <input type="button" class="btnLogin right" id="btnSendMsg" value="获取验证码" style="width:100px;font-size:14px;"/>

                    <div class="clear"></div>

                    <div class="error-body" id="code-error">
                        <div class="error-lump"></div>
                        <div class="error-msg"></div>
                    </div>
                </div>

                 <div class="loginbar">
                    <input class="input icoUserCompany" id="companyName" type="text" placeholder="公司名称" value="" maxlength="25"/>
                     <div class="error-body">
                        <div class="error-lump"></div>
                        <div class="error-msg"></div>
                    </div>
                </div>

                <div class="loginbar">
                    <input class="input icoName" type="text" id="name" placeholder="姓名" value="" maxlength="25"/>
                    <div class="error-body">
                        <div class="error-lump"></div>
                        <div class="error-msg"></div>
                    </div>
                </div>

                <div class="loginbar relative">
                    <input class="input icoUserPwd" id="loginPWD" type="password" maxlength="25"/>
                    <div class="error-body">
                        <div class="error-lump"></div>
                        <div class="error-msg"></div>
                    </div>
                    <span class="txtBoxPassword" id="txtBoxPassword">密码,6-25位,至少包含字母/数字/符号两种不同组合</span>
                </div>

                <div class="loginbar relative">
                    <input class="input icoSurePwd" id="loginSurePWD" type="password" maxlength="25"/>
                    <div class="error-body">
                        <div class="error-lump"></div>
                        <div class="error-msg"></div>
                    </div>
                    <span class="txtBoxPassword" id="txtBoxSurePassword">确认密码</span>
                </div>

                 <div class="remember-box">
                    <div class="cb-remember-password">
                         注册即同意<a href="/home/terms/" target="_blank">《智能工厂服务与授权协议》</a>
                    </div>
                    
                </div>

                <div class="btnLogin" id="btnRegister">注册</div>
            </div>
        </div>

        <div class="copyright">
            Copyright©2016-2017，备案号：浙ICP16001314  技术支持 <a href="http://www.yunxiaokeji.com/">云销科技</a>
        </div>
    </div>
</body>
</html>
