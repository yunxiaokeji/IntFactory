@{
    ViewBag.Title = "我的任务";
    Layout = "/Views/Shared/_LayoutNoLeft.cshtml";
}

@section css{
    @*<link rel="stylesheet" href="/modules/css/system/organization.css" />*@
}
@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/task/task"], function (obj) {
            obj.init();
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">我的任务</span>
    @*<div class="btn-add right @(ExpandClass.IsLimits(HttpContext.Current,"108010101"))" id="createModel">新建部门</div>*@
</div>
<div class="content-body mTop20">
    <table class="table-list">
        <tr class="tr-header">
            <td class="tLeft">部门名称</td>
            <td class="tLeft">描述</td>
            <td class="width150">创建时间</td>
            <td class="width80">操作</td>
        </tr>
    </table>

    <div id="pager" class="mTop20"></div>
</div>



<ul class="dropdown-ul hide"  style="width:65px;">
    <li class="@(ExpandClass.IsLimits(HttpContext.Current,"108010101"))" id="updateObject">编辑</li>
    <li class="@(ExpandClass.IsLimits(HttpContext.Current,"108010102"))" id="deleteObject">删除</li>
</ul>
