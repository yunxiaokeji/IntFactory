
@{
    ViewBag.Title = "个人中心";
    Layout = "/Views/Shared/_LayoutUserCenter.cshtml";
}

@section css{
    <link href="/modules/css/myaccount/myaccount.css" rel="stylesheet" />
    <link href="/modules/css/myaccount/feedbacklist.css" rel="stylesheet" />
    <link href="/modules/plug/daterangepicker/daterangepicker.css" rel="stylesheet" />
}

@section scripts{
<script type="text/javascript">
    seajs.use(["scripts/myaccount/feedbacklist"], function (ObjectJS) {
        ObjectJS.init();
    });
</script>
}
<div class="accountContent">
    <div class="accountHeader">反馈信息</div>

    <div class="content-body mTop20 pBottom50">
        <div class="search-body">
            <ul class="search-item search-feedbacktype">
                <li class="column-name">类型：</li>
                <li data-type="-1" class="item hover">全部</li>
                <li data-type="1" class="item">问题</li>
                <li data-type="2" class="item">意见</li>
                <li data-type="3" class="item">需求</li>
            </ul>
        </div>

        <div class="search-items mTop20">
            <div class="left mRight20" id="chooseBranch"></div>
                <div class="left">
                    <input type="text" id="iptCreateTime" placeholder="反馈日期"  class="ipt-date"  />
                </div>
            <div class="right searth-module" data-text="关键字搜索..." data-width="260"> 
            </div>
        </div>

        <ul class="search-sort">
            <li>排序</li>
            <li class="sort-item hover" data-isasc="0" data-isactive="1" data-orderbycloumn="0">
                反馈时间
                <span class="asc"></span>
                <span class="desc hover"></span>
            </li>
        </ul>

        <table class="table-list mTop20">
            <tr class="tr-header">
                <td class="tLeft">标题</td>
                <td>类型</td>
                <td>反馈时间</td>
                <td>回答</td>
                <td>状态</td></tr>
        </table>

        <div id="pager" class="mTop20"></div>
    </div>
</div>


