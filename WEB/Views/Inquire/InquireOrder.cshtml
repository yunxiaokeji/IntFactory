@{    
    ViewBag.Title = "InquireOrder";
    Layout = null;
}

<!doctype html>
<html>
<head>
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta http-equiv="content-language" content="zh-CN" />
	<meta name="author" content="zuroke" />
	<title>查询订单进度</title>
    <link href="/modules/css/default/layout.css" rel="stylesheet" />
    <link href="/modules/css/base.css" rel="stylesheet" />
    <link href="/modules/css/inquire/common.css" rel="stylesheet" />    
    <link href="/modules/css/inquire/online-pay.css" rel="stylesheet" type="text/css" />

    <script src="/Scripts/jquery-1.11.1.js"></script>
    <script src="/Scripts/sea.js"></script>
    <script src="/Scripts/sea-config.js"></script>
        
    <script type="text/javascript">        
        seajs.use("/modules/scripts/inquire/inquireorder", function (e) {
            e.init();
        });       
    </script>
</head>

<body style="background-color:#fff !important;">
<!--页头-->
<div class="pay-title pay-body row">
        
    <div class="pay-title-name l" onclick="location.href='http://www.intfactory.cn/'">
        <i><img src="/modules/images/logo_login.png" alt=""/></i>
        <span>厂盟智能</span>
    </div>
</div>
<!--页头end-->

<!--内容-->
<div class="pay-content pay-body row">
	<!---查询框--->
    <div class="inquire">
    	<div class="inquire-title">查询订单进度</div>
        
        <div class="inquire-form">
        	<ul>
            	<li>
                    <input type="text" class="inquire-form-phone" maxlength="11" placeholder="输入手机获取验证码" />
                </li>
                <li>
                    <input type="text" class="inquire-form-numb" maxlength="6" placeholder="输入验证码" />
                    <input type="button" class="inquire-form-btn" value="获取验证码" />
                </li>
                <li>
                	<input type="button" value="查询进度" disabled="disabled" class="inquire-form-button" />
                </li>
            </ul>
        </div>
    </div>
    <!---查询框end--->
    
    <!---订单状态信息--->
    <div class="info"><div class="infobox row"><div class="info-none"></div></div></div>
    
    <!---订单状态信息end--->

    
</div>
<!--内容end-->

<!--页脚-->
<div class="pay-bottom pay-body row">
	<p>©2015-2016 智能工厂 沪ICP备15042543号</p>
</div>
<!--页脚end-->
</body>
</html>

