@{
    ViewBag.Title = "员工工量统计";
}
 <link href="/modules/plug/daterangepicker/daterangepicker.css" rel="stylesheet" />
@section scripts{
    <script src="/Scripts/jquery-1.11.1.js"></script>
    <script type="text/javascript">
        seajs.use(["scripts/report/userworkload"], function (obj) {
            obj.init();
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">员工工量统计</span>
</div>
<div class="search-header">
    <div class="left" title="日期">
        <input type="text" id="iptCreateTime" placeholder="日期" class="ipt-date"  />
    </div>
</div>
<div class="content-body pBottom50">
    <table id="userTotalRPT" class="table-list">
        <tr class="tr-header">
            <td class="tLeft">员工</td>
            <td>总工量</td>
            <td>裁片（织机）</td>
            <td>车缝（缝盘）</td>
            <td>车缝（缝盘）退回</td>
        </tr>
    </table>
</div>

