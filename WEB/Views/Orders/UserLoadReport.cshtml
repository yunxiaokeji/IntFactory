@{
    ViewBag.Title = "员工工量统计";
}
 <link href="/modules/plug/daterangepicker/daterangepicker.css" rel="stylesheet" />
@section scripts{
    <script src="/Scripts/jquery-1.11.1.js"></script>
    <script type="text/javascript">
        seajs.use(["scripts/report/userworkload"], function (obj) {
            obj.init();
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">员工工量统计</span>
</div>
<div class="content-body pBottom50">
    <div class="search-items">
        <div class="left mRight20" id="chooseBranch"></div>
        <div class="left">
            <input type="text" id="iptCreateTime" placeholder="交货日期"  class="ipt-date"  />
        </div>
    </div>
    <table id="userTotalRPT" class="table-list-rpt">
        <tr class="tr-header">
            <td class="tLeft" style="width:20%;">员工</td>
            <td style="width:20%;">总工量</td>
            <td style="width:20%;">裁片（织机）</td>
            <td style="width:20%;">车缝（缝盘）</td>
            <td style="width:20%;">车缝（缝盘）退回</td>
        </tr>
    </table>
</div>

