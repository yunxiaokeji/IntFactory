@{
    ViewBag.Title = "订单生产数据统计";
}

 <link href="/modules/plug/daterangepicker/daterangepicker.css" rel="stylesheet" />
@section scripts{
    <script src="/Scripts/jquery-1.11.1.js"></script>
    <script type="text/javascript">
        seajs.use(["scripts/report/orderproductionreport"], function (obj) {
            obj.init();
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">订单生产数据统计</span>
</div>
<div class="content-body pBottom50">
    <div class="search-items">
        <div class="left mRight20" id="chooseBranch"></div>
        <div class="left" id="ddlTimeType"></div>
        <div class="left mLeft10">
            <input type="text" id="iptCreateTime" placeholder="统计日期"  class="ipt-date"  />
        </div>
         <div class="right searth-module" data-text="输入款式编码/客户名称..." data-width="200"> </div>
    </div>
    <table id="userTotalRPT" class="table-list-rpt">
        <tr class="tr-header">
            <td class="tLeft" style="width:10%;">款式编码</td>
            <td style="width:10%;">负责人</td>
            <td style="width:10%;">客户</td>
            <td style="width:10%;">下单数</td>
            <td style="width:10%;">裁剪数</td>
            <td style="width:10%;">车缝数</td>
            <td style="width:10%;">发货数</td>
            <td style="width:10%;">剩余未发</td>
            <td style="width:10%;">单价</td>
            <td style="width:10%;">总金额</td>
        </tr>
    </table>
</div>
