@{
    
    ViewBag.Title = "员工车缝（缝盘）统计";
}
 <link href="/modules/plug/daterangepicker/daterangepicker.css" rel="stylesheet" />
@section scripts{
    <script src="/Scripts/jquery-1.11.1.js"></script>
    <script type="text/javascript">
        seajs.use(["scripts/report/userworkload"], function (obj) {
            obj.init();
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">
       员工车缝（缝盘）统计
    </span>
</div>
<div class="search-header">
    <ul class="search-tab search-reporttype">
            <li data-id="1" class="item hover" data-nav="userTotalRPT">整件成品</li>
            <li data-id="2" class="item" data-nav="userProcessRPT">工序统计</li>
        </ul>
</div>
<div class="content-body pBottom50">
    <div class="search-items">
        <div class="left mRight20" id="chooseBranch"></div>
        <div class="left">
            <input type="text" id="iptCreateTime" placeholder="统计日期"  class="ipt-date"  />
        </div>
    </div>
    <table id="userTotalRPT" class="table-list-rpt">
        <tr class="tr-header">
            <td class="tLeft" style="width:10%;">员工</td>
            <td style="width:10%;">车缝（缝盘）数量</td>
            <td style="width:10%;">车缝（缝盘）退回</td>
        </tr>
    </table>
    <table id="userProcessRPT" class="table-list-rpt hide">
        
    </table>
</div>

