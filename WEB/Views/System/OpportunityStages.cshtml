@{
    ViewBag.Title = "销售机会阶段";
}

@section css{
    <link rel="stylesheet" href="/modules/css/system/system.css" />
}
@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/system/opportunitystage"], function (obj) {
            obj.init();
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">销售机会阶段</span>
    <div class="btn-add right @(ExpandClass.IsLimits(HttpContext.Current,"108020801"))" id="createModel">新建阶段</div>
</div>
<div class="content-body mTop20">
    <table class="table-list">
        <tr class="tr-header">
            <td class="tLeft">阶段名称</td>
            <td class="tLeft">成交概率(%)</td>
            <td class="width150">创建时间</td>
            <td class="width150">创建人</td>
            <td class="width80">操作</td>
        </tr>
    </table>
</div>
<ul class="dropdown-ul hide"  style="width:65px;">
    <li class="@(ExpandClass.IsLimits(HttpContext.Current,"108020801"))" id="updateObject">编辑</li>
    <li class="@(ExpandClass.IsLimits(HttpContext.Current,"108020802"))" id="deleteObject">删除</li>
</ul>
