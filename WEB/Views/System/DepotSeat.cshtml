@{
    var model = (IntFactoryEntity.WareHouse)ViewBag.Ware;
    ViewBag.Title = model.Name+"—货位设置";
    
}

<link href="/modules/css/system.css" rel="stylesheet" />
@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/system/depotseat"], function (obj) {
            obj.init('@(model.WareID)');
        });
    </script>
}
<div class="header-box">
    <a class="btn-back right" href="javascript:if(history.length>1){ history.go(-1);} else{}">返回 </a>
    <a href="javascript:void(0);" class="btn-add right mRight10">新建货位</a>
</div>

<div class="content-body mTop20 pBottom50">
    <table class="table-list">
        <tr class="tr-header" id="warehouse-items">
            <td class="width150 tLeft">货位编码</td>
            <td class="width100">优先（高）</td>
            <td class="width100">优先（低）</td>
            <td class="width100">启用/禁用</td>
            <td class="width300 tLeft">描述</td>
            <td class="width100">操作</td>
        </tr>
    </table>
    <div id="pager" class="mTop20"></div>
</div>
<ul class="dropdown-ul hide"  style="width:65px;">
    <li class="" id="updateObject">编辑</li>@*@(ExpandClass.IsLimits(HttpContext.Current,"108020604"))*@
    <li class="" id="deleteObject">删除</li>@*@(ExpandClass.IsLimits(HttpContext.Current,"108020606"))*@
</ul>
