@{
    ViewBag.Title = "属性（规格）详情";
}

<link href="/modules/css/products.css" rel="stylesheet" />

@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/products/attr"], function (attr) {
            attr.initDetail('@(ViewBag.AttrID)');
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">属性（规格)</span>
    <a href="javascript:void(0)" id="addAttrValue" class="btn-add right">新建属性值</a>
    <a class="btn-back right mRight10" href="javascript:if(history.length>1){ history.go(-1);} else{}">返回 </a> 
</div>
<div class="content-body mTop10">
    <table class="table-list">
        <tr class="tr-header" id="attrItems">
            <td class="tLeft">属性值名称</td>
            <td class="width150">排序</td>
            <td class="width60 tRight">操作</td>
        </tr>
    </table>
</div>

