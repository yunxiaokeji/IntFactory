@{
    ViewBag.Title = "客户列表";
}

@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/client/clients"], function (client) {
            client.init();
        });
    </script>
}

<div class="header-box">
    <span class="left header-title">客户列表</span>
    <a href="Create" class="btn right  @(ExpandClass.IsLimits(HttpContext.Current,"201010101"))">新建客户</a>
</div>

<div class="content-body">
    <div id="ClientType" class="left"></div>
    <div class="right searth-module"> </div>
    <div class="clear"></div>
    <table class="table-list mTop10">
        <tr class="tr-header" id="client-header">    
            <td class="tLeft">工厂编码</td>        
            <td class="tLeft">工厂名称</td>
            <td class="tLeft">联系人</td>
            <td class="tLeft">移动电话</td>
            <td class="tLeft">省份城市</td>
            <td class="">
                 <span class="td-span" data-column="CreateTime"> 注册日期
                    <span class="asc"></span>
                    <span class="desc"></span>
               </span>
            </td>
            <td class="">
                <span class="td-span" data-column="UserQuantity"> 授权人数
                    <span class="asc"></span>
                    <span class="desc"></span>
               </span>
            </td>
            <td class="">
                <span class="td-span" data-column="EndTime"> 到期时间
                    <span class="asc"></span>
                    <span class="desc"></span>
               </span>
             </td>
        </tr>
    </table>

    <div id="pager" class="mTop20"></div>
</div>

