@{
    ViewBag.Title = "客户订单列表";
}
@section css{
    <link href="/modules/plug/daterangepicker/daterangepicker.css" rel="stylesheet" />
    }
<script src="/modules/plug/laydate/laydate.js"></script>
@section scripts{    
    <script type="text/javascript">
        seajs.use(["scripts/client/order"], function (order) {
            order.init();
        }); 
    </script>
}

<div class="header-box">
    <span class="header-title">客户订单列表</span>
</div>
<div class="content-body">
    <div class="left mLeft5">
         <input type="text" id="orderBeginTime" placeholder="请选择时间段"  class="ipt-date" /> 
    </div>
    <div id="OrderTypes" class="left mLeft20"></div>
    <div id="OrderStatus" class="left mLeft20"></div>
     <div class="right searth-module"> </div>
    <div class="clear"></div>

    <table class="table-list mTop20">
        <tr class="tr-header" id="clientOrders">
            <td class="width80 tLeft">订单类型</td>  
            <td class="width80">授权人数</td>
            <td class="width80">购买年数</td>
            <td class="width150">金额</td>
            <td class="width150">订单状态</td>  
            <td class="width100">支付状态</td>  
            <td class="tLeft width100">客户编码</td>  
            <td class="tLeft width200">客户名称</td>
            <td class=" minwidth70">订单时间</td>   
            <td class="width100">创建人</td>
            <td class="width150">来源</td>
            <td class="width150">操作</td>
        </tr>
    </table>
     <div id="pager" class="mTop20"></div>
</div>
<ul class="dropdown-ul hide" style="width:65px;">
    <li class="@(ExpandClass.IsLimits(HttpContext.Current,"201010201"))" id="examineOrder">审核</li>
    <li class="@(ExpandClass.IsLimits(HttpContext.Current,"201010202"))" id="editOrder">改价</li>
    <li class="@(ExpandClass.IsLimits(HttpContext.Current,"201010203"))" id="deleteOrder">删除</li>
</ul>