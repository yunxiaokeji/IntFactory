@{
    ViewBag.Title = "平台收费列表";
}

@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/system/modulesproduct"], function (ModulesProduct) {
            ModulesProduct.init();
        });
    </script>
}
<div class="header-box">
    <span class="header-title">平台收费列表</span>
    <a class="btn right @(ExpandClass.IsLimits(HttpContext.Current,"204010501"))" href="Detail">新建收费产品</a>
</div>

<div class="content-body">
    <div id="PeriodQTY" class="left"></div>
    <div class="clear"></div>
    <table class="table-list mTop10" id="productPeriod">
        <tr class="tr-header">            
            <td class="tLeft width150">周期量</td>
            <td class="width150">人数</td>
            <td class="width150">价格</td>
            <td class="tLeft">描述</td>
            <td class="width150">创建时间</td>
            <td class="width80">操作</td>
        </tr>
    </table>
    <div id="pager" class="mTop20"></div>
</div>
