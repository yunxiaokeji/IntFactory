@{
    ViewBag.Title = "快递公司";
}

@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/system/expresscompany"], function (ExpressCompany) {
            ExpressCompany.detailInit('@ViewBag.ExpressID');
        });
    </script>
}
<link href="/modules/css/clients.css" rel="stylesheet" />

<div class="header-box">
    <span class="header-title">添加快递公司</span>
    @*<a class="btn right" href="javascript:history.go(-1);">返回</a>*@
     <a class="btn-back right" href="javascript:if(history.length>1){ history.go(-1);} else{}">返回</a> 
</div>

<div class="content-body">
    <ul class="contentnew" style="display:block;">
        <li>
            <span class="columnname">名称：</span>
            <span><input type="text" id="Name" class="verify"  maxlength="11"  data-empty="名称不能为空!" /></span>
        </li>
         <li>
            <span class="columnname">网址：</span>
            <span><input type="text" id="Website" style="width:270px;" class="verify"  data-empty="网址不能为空!" /></span>
        </li>
    </ul>

    <div class="mTop20">
        <div id="saveExpressCompany" class="btn">添加</div>
        <div @(ExpandClass.IsLimits(HttpContext.Current,"204010303"))" id="delExpressCompany" class="btn" style="display:none">删除</div>
    </div>
</div>
    
