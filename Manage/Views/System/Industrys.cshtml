@{
    ViewBag.Title = "行业列表";
}

@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/system/industry"], function (Industry) {
            Industry.init();
        });
    </script>
}

<div class="header-box">
    <span class="header-title">行业列表</span>
    <a class="btn right  @(ExpandClass.IsLimits(HttpContext.Current,"204010202"))" href="IndustryDetail">新建行业</a>
</div>

<div class="content-body ">
    <div class="right searth-module"> </div>
    <div class="clear"></div>
    <table class="table-list mTop10">
        <tr class="tr-header">            
            <td class="tLeft">名称</td>
            <td class="tLeft">描述</td>
            <td class="width150">创建时间</td>
            <td class="width80">操作</td>
        </tr>
    </table>
</div>
