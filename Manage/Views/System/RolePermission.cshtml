@{
    ViewBag.Title = "权限设置";
    var menus = (List<IntFactoryEntity.Menu>)ViewBag.Menus;
    var model = (IntFactoryEntity.Manage.M_Role)ViewBag.Model;
}

@section css{
    <link rel="stylesheet" href="/modules/css/system/role.css" />
}
@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/system/roles"], function (obj) {
            obj.initPermission('@(model.RoleID)', '@Html.ToJSONString(menus)', '@Html.ToJSONString(model.Menus)');
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">@(model.Name)-权限设置</span>
     <a class="btn-back right" href="javascript:if(history.length>1){ history.go(-1);} else{}">返回</a> 
</div>
<div class="content-body">
    <div id="rolePermission" class="menulist">

    </div>
    <div class="mTop20">
        <div class="btn @(ExpandClass.IsLimits(HttpContext.Current,"204020203"))" id="savePermission">保存</div>
    </div>
</div>
